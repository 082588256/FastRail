@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <style>
        .train-search-container {
            background: #fff;
            padding: 30px;
            border-radius: 12px;
            max-width: 600px;
            margin: 0 auto;
            box-shadow: 0 0 20px rgba(0,0,0,0.1);
            font-family: Arial, sans-serif;
        }

        .tabs {
            display: flex;
            justify-content: center;
            margin-bottom: 20px;
        }

            .tabs .tab {
                background: #fce4ec;
                color: #d81b60;
                padding: 10px 20px;
                border-radius: 20px;
                font-weight: bold;
                font-size: 16px;
            }

        .search-form {
            display: flex;
            flex-direction: column;
            gap: 15px;
        }

        .form-row label {
            display: block;
            margin-bottom: 5px;
            font-weight: bold;
        }

        .form-row input,
        .form-row select {
            width: 100%;
            padding: 10px;
            border: 1px solid #ccc;
            border-radius: 8px;
            font-size: 15px;
        }

        .search-btn {
            margin-top: 10px;
            background-color: #000; /* đã sửa từ #ff4081 thành đen */
            color: white;
            padding: 12px;
            border: none;
            border-radius: 10px;
            font-size: 16px;
            cursor: pointer;
            transition: background 0.3s;
            font-weight: bold;
        }

            .search-btn:hover {
                background-color: #333; /* màu đen nhạt khi hover */
            }

    </style>
</head>

<div class="main-banner-wrapper">
    <section class="banner-style-one owl-theme owl-carousel no-dots">
        <div class="slide slide-one" style="background-image: url(images/slider/slider-1-1.jpg);">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h3 class="banner-title">Cheap & Trusted <br> FastRail Company</h3>
                        <p>Enjoy your comfortable trip with <br> company FastRail</p>
                        <div class="btn-block">
                            <a href="#" class="banner-btn">Learn More</a>
                        </div><!-- /.btn-block -->
                    </div><!-- /.col-lg-12 -->
                </div><!-- /.row -->
            </div><!-- /.container -->
        </div><!-- /.slide -->
        <div class="slide slide-two" style="background-image: url(images/slider/slider-1-2.jpg);">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 text-center">
                        <h3 class="banner-title">Cheap & Trusted <br> FastRail Company</h3>
                        <p>Enjoy your comfortable trip with <br> conexi company taxi</p>
                        <div class="btn-block">
                            <a href="#" class="banner-btn">Learn More</a>
                        </div><!-- /.btn-block -->
                    </div><!-- /.col-lg-12 -->
                </div><!-- /.row -->
            </div><!-- /.container -->
        </div><!-- /.slide -->
    </section><!-- /.banner-style-one -->
    <div class="carousel-btn-block banner-carousel-btn">
        <span class="carousel-btn left-btn"><i class="conexi-icon-left"></i></span>
        <span class="carousel-btn right-btn"><i class="conexi-icon-right"></i></span>
    </div><!-- /.carousel-btn-block banner-carousel-btn -->
</div><!-- /.main-banner-wrapper -->
<section class="book-ride-one">
    @* <img src="~/images/background/booking-bg-1-1.png" class="footer-bg" alt="Awesome Image" /> *@
    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <div class="content-block">
                    <div class="block-title mb-0">
                        <div class="dot-line"></div><!-- /.dot-line -->
                        <p class="light">Looking for trip?</p>
                        <h2 class="light">Search your <br> Trip</h2>
                    </div><!-- /.block-title -->
                    <p>Our trains commit to make your trips unique <br> by best answering your needs.</p>
                </div><!-- /.content-block -->
            </div><!-- /.col-lg-4 -->
                <form action="/Booking/TrainView" method="post" class="booking-form-one">
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="input-holder">
                                <label>Ga đi:</label>
                                <select class="form-control" name="DepartureStationName">
                                    @foreach (var item in ViewBag.Stations as List<SelectListItem>)
                                    {
                                        <option value="@item.Text">@item.Text</option>
                                    }
                                </select>
                            </div><!-- /.input-holder -->
                        </div><!-- /.col-lg-6 -->
                        <div class="col-lg-3">
                            <div class="input-holder">
                                <label>Ga đến:</label>
                                <select class="form-control" name="ArrivalStationName">
                                    @foreach (var item in ViewBag.Stations as List<SelectListItem>)
                                    {
                                        <option value="@item.Text">@item.Text</option>
                                    }
                                </select>
                            </div><!-- /.input-holder -->
                        </div><!-- /.col-lg-6 -->
                        <div class="col-lg-5">
                            <div class="input-holder">
                                <label>Ngày khởi hành </label>
                                <select class="form-control" name="TravelDate">
                                    @{
                                        var today = DateTime.Today;
                                        for (int i = 0; i < 3; i++) // 7 ngày gần nhất kể từ hôm nay
                                        {
                                            var date = today.AddDays(i);
                                        }
                                    }
                                    @for (int i = 0; i < 7; i++)
                                    {
                                        var date = today.AddDays(i);
                                        <option value="@date.ToString("yyyy-MM-dd")">
                                            @date.ToString("dddd, dd/MM/yyyy") <!-- Thứ, ngày/tháng/năm -->
                                        </option>
                                    }
                                </select>
                            </div><!-- /.input-holder -->
                        </div><!-- /.col-lg-6 -->
                        <div class="col-lg-3">
                            <div class="input-holder">
                                <button type="submit">Search</button>
                            </div><!-- /.input-holder -->
                        </div><!-- /.col-lg-6 -->
                    </div><!-- /.row -->
                </form>
            </div><!-- /.col-lg-8 -->
        </div><!-- /.row -->
    </div><!-- /.container -->
</section><!-- /.book-ride-one -->
