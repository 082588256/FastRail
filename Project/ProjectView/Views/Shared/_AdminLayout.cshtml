<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Dashboard</title>

    <!-- Fontfaces CSS-->
    <link href="@Url.Content("~/css/font-face.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/font-awesome-4.7/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/font-awesome-5/css/fontawesome-all.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/mdi-font/css/material-design-iconic-font.min.css")" rel="stylesheet" />
    

    <!-- Bootstrap CSS-->
    <link href="@Url.Content("~/vendor/bootstrap-4.1/bootstrap.min.css")" rel="stylesheet" />

    <!-- Vendor CSS-->
    <link href="@Url.Content("~/vendor/animsition/animsition.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/wow/animate.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/css-hamburgers/hamburgers.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/slick/slick.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/select2/select2.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/vendor/perfect-scrollbar/perfect-scrollbar.css")" rel="stylesheet" />

    <!-- Main CSS-->
    <link href="@Url.Content("~/css/theme.css")" rel="stylesheet" />


</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="/Admin">
                            <img src="~/images/icon/logo.png" alt="CoolAdmin" />
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>BÁO CÁO
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a href="/Admin/Index">BÁO CÁO TUYẾN, TÀU</a>
                                </li>
                                <li>
                                    <a href="/Admin/Revenue">BÁO CÁO DOANH THU</a>
                                </li>
                            </ul>
                            
                        </li>
                        <li>
                            <a href="RouteManagement">
                                <i class="fas fa-chart-bar"></i>Route Management</a>
                        </li>
                        
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->

        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="images/icon/logo.png" alt="Cool Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li class="has-sub">
                            <a class="js-arrow" href="#">
                                <i class="fas fa-tachometer-alt"></i>BÁO CÁO
                            </a>
                            <ul class="navbar-mobile-sub__list list-unstyled js-sub-list">
                                <li>
                                    <a href="/Admin/Index">BÁO CÁO TUYẾN, TÀU</a>
                                </li>
                                <li>
                                    <a href="/Admin/Revenue">BÁO CÁO DOANH THU</a>
                                </li>
                                
                            </ul>

                        </li>
                        <li>
                            <a href="/RouteManagement">
                                <i class="fas fa-chart-bar"></i>QUẢN LÝ TUYẾN</a>
                        </li>
                        
                        
                        
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>
    
    <script src="@Url.Content("~/vendor/jquery-3.2.1.min.js")"></script>
    <script src="@Url.Content("~/vendor/bootstrap-4.1/popper.min.js")"></script>
    <script src="@Url.Content("~/vendor/bootstrap-4.1/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/vendor/slick/slick.min.js")"></script>
    <script src="@Url.Content("~/vendor/wow/wow.min.js")"></script>
    <script src="@Url.Content("~/vendor/animsition/animsition.min.js")"></script>
    <script src="@Url.Content("~/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js")"></script>
    <script src="@Url.Content("~/vendor/counter-up/jquery.waypoints.min.js")"></script>
    <script src="@Url.Content("~/vendor/counter-up/jquery.counterup.min.js")"></script>
    <script src="@Url.Content("~/vendor/circle-progress/circle-progress.min.js")"></script>
    <script src="@Url.Content("~/vendor/perfect-scrollbar/perfect-scrollbar.js")"></script>
    <script src="@Url.Content("~/vendor/chartjs/Chart.bundle.min.js")"></script>
    <script src="@Url.Content("~/vendor/select2/select2.min.js")"></script>
    <script src="@Url.Content("~/js/main.js")"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>
<!-- end document-->
